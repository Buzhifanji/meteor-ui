{
  "name": "vanilla-ui",
  "version": "1.0.0",
  "description": "web components ui",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "dev": "set NODE_ENV=developemnt&& rollup -c rollup.config.js -w",
    "build": "rm -fr lib && set NODE_ENV=production&& rollup -c rollup.config.js",
    "test": "tsc && web-test-runner  \"out-tsc/**/*.test.{html,js}\" --node-resolve --coverage  --playwright --browsers chromium firefox webkit",
    "test:watch": "web-test-runner \"test/**/*.test.ts\" --node-resolve --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4-fix.0",
    "@open-wc/testing": "^3.0.0-next.5",
    "@types/bluebird": "^3.5.36",
    "@types/node": "^17.0.5",
    "@typescript-eslint/parser": "^5.8.1",
    "@web/test-runner": "^0.13.23",
    "@web/test-runner-commands": "^0.6.0",
    "@web/test-runner-mocha": "^0.7.5",
    "@web/test-runner-playwright": "^0.8.8",
    "@web/test-runner-puppeteer": "^0.10.4",
    "eslint": "^8.0.0",
    "rollup": "^2.61.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.5.4"
  }
}