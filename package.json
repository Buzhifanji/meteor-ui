{
  "private": true,
  "name": "one-ui",
  "scripts": {
    "start": "pnpm run start --filter '*'",
    "reinstall": "pnpm clean && pnpm install",
    "dumi": "dumi dev",
    "build:dumi": "dumi build",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "dev": "vite --config scripts/vite.config.js",
    "lib": "vite build --config scripts/vite.lib.config.js",
    "lib:watch": "vite build --config scripts/vite.lib.config.js --watch",
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "clean": "rm -rf node_modules **/*/node_modules",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.35",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/node": "^17.0.17",
    "dumi": "^1.1.38",
    "jest": "^27.5.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "react": "^16.14.0",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.0",
    "svelte": "^3.46.2",
    "svelte-check": "^2.3.0",
    "svelte-jester": "^2.3.1",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.10.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "yorkie": "^2.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "moduleNameMapper": {
      "^utils/(.*)$": "<rootDir>/utils/$1"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testMatch": [
      "<rootDir>/packages/**/*.test.ts"
    ]
  }
}
